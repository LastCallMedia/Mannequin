{
    "name": "lastcallmedia/patterns",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Bayliss",
            "email": "rob@lastcallmedia.com"
        }
    ],
    "require": {
        "symfony/console": "^2.7 ||^3.0",
        "pimple/pimple": "^3.0",
        "symfony/filesystem": "^2.7 ||^3.0",
        "symfony/templating": "^2.7 ||^3.0",
        "silex/silex": "^2.0",
        "symfony/web-server-bundle": "dev-master",
        "symfony/finder": "^2.7 ||^3.0",
        "twig/twig": "^1.3.0",
        "symfony/yaml": "^2.7 ||^3.0"
    },
    "autoload": {
      "psr-4": {
        "LastCall\\Patterns\\Core\\": "core/",
        "LastCall\\Patterns\\Cli\\": "cli/",
        "LastCall\\Patterns\\Twig\\": "twig/",
        "LastCall\\Patterns\\Drupal\\": "drupal/"
      },
      "exclude-from-classmap": [
        "**/Tests/"
      ]
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
  "bin": [
    "cli/bin/patterns"
  ]
}
