{
    "name": "lastcallmedia/mannequin",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Bayliss",
            "email": "rob@lastcallmedia.com"
        }
    ],
    "require": {
        "php": ">7.0",
        "symfony/console": "^2.7 ||^3.0",
        "pimple/pimple": "^3.0",
        "symfony/filesystem": "^2.7 ||^3.0",
        "symfony/process": "^2.7 ||^3.0",
        "silex/silex": "^2.0",
        "symfony/finder": "^2.7 ||^3.0",
        "twig/twig": "^1.3.0",
        "symfony/yaml": "^2.7 ||^3.0",
        "symfony/cache": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "LastCall\\Mannequin\\Core\\": "core/",
            "LastCall\\Mannequin\\Cli\\": "cli/",
            "LastCall\\Mannequin\\Html\\": "html/",
            "LastCall\\Mannequin\\Twig\\": "twig/",
            "LastCall\\Mannequin\\Drupal\\": "drupal/"
        },
        "exclude-from-classmap": [
            "**/Tests/"
        ]
    },
    "bin": [
        "cli/bin/patterns"
    ]
}
