{
    "name": "lastcall/mannequin-core",
    "description": "An open source template viewer",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Bayliss",
            "email": "rob@lastcallmedia.com"
        }
    ],
    "support": {
        "issues": "https://github.com/LastCallMedia/Mannequin/issues",
        "source": "https://github.com/LastCallMedia/Mannequin",
        "docs": "https://mannequin.io"
    },
    "require": {
        "php": ">7.3",
        "lastcall/composer-extra-files": "~1.0",
        "symfony/console": "^3.0 ||^4.4",
        "symfony/filesystem": "^3.0 ||^4.4",
        "symfony/process": "^3.0 ||^4.4",
        "symfony/expression-language": "^3.0 ||^4.4",
        "pimple/pimple": "^3.0",
        "symfony/yaml": "^3.0 ||^4.4",
        "psr/cache": "~1.0",
        "symfony/asset": "^3.0 ||^4.4",
        "symfony/cache": "^3.3 ||^4.4",
        "sebastian/version": "^1.0 || ^2.0",
        "symfony/http-foundation": "^3.4 ||^4.4",
        "symfony/http-kernel": "^3.4 ||^4.4",
        "symfony/routing": "^3.4 ||^4.4",
        "symfony/web-link": "^3.4 ||^4.4",
        "symfony/finder": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "LastCall\\Mannequin\\Core\\": ""
        },
        "exclude-from-classmap": [
            "/Tests"
        ]
    },
    "bin": [
        "bin/mannequin"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-composer_download": "2.0-dev"
        },
        "extra-files": {
            "ui": {
                "url": "https://registry.npmjs.org/lastcall-mannequin-ui/-/lastcall-mannequin-ui-1.0.7.tgz",
                "path": "ui-files"
            }
        }
    }
}
