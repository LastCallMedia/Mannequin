{
    "name": "lastcall/mannequin-core",
    "description": "An open source template viewer",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Bayliss",
            "email": "rob@lastcallmedia.com"
        }
    ],
    "require": {
        "php": ">7.0",
        "lastcall/composer-extra-files": "~1.0",
        "symfony/console": "^2.7 ||^3.0",
        "symfony/filesystem": "^2.7 ||^3.0",
        "symfony/process": "^2.7 ||^3.0",
        "symfony/expression-language": "^2.7 ||^3.0",
        "silex/silex": "^2.0",
        "pimple/pimple": "^3.0",
        "symfony/yaml": "^2.7 ||^3.0",
        "psr/cache": "~1.0",
        "symfony/asset": "^2.7 || ^3.0",
        "symfony/cache": "^3.3",
        "sebastian/version": "^1.0 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "LastCall\\Mannequin\\Core\\": ""
        },
        "exclude-from-classmap": [
            "/Tests"
        ]
    },
    "bin": [
        "bin/mannequin"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-composer_download": "2.0-dev"
        },
        "extra-files": {
            "ui": {
                "url": "https://registry.npmjs.org/lastcall-mannequin-ui/-/lastcall-mannequin-ui-1.0.3.tgz",
                "path": "ui-files"
            }
        }
    }
}
