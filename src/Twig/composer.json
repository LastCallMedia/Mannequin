{
    "name": "lastcall/mannequin-twig",
    "description": "Twig extension for the Mannequin template viewer",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Bayliss",
            "email": "rob@lastcallmedia.com"
        }
    ],
    "require": {
        "php": ">7.0",
        "lastcall/mannequin-core": "~0.1",
        "symfony/finder": "^2.7 ||^3.0",
        "twig/twig": "^1.3.0",
        "psr/cache": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "LastCall\\Mannequin\\Twig\\": ""
        },
        "exclude-from-classmap": [
            "/Tests"
        ]
    }
}
