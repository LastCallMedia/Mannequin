{
  "name": "lastcall-mannequin-ui",
  "version": "0.2.2",
  "license": "MIT",
  "author": "Rob Bayliss <rob@lastcallmedia.com>",
  "devDependencies": {
    "classnames": "^2.2.5",
    "foundation-sites": "^6.4.0",
    "node-sass-chokidar": "^0.0.1",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.5.3",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-fluid-container": "^0.6.0",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-scripts": "^1.0.0",
    "react-styleguidist": "^6.0.13",
    "react-syntax-highlighter": "^5.6.2",
    "react-transition-group": "^2.2.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "source-map-explorer": "^1.4.0",
    "what-input": "^4.3.0"
  },
  "scripts": {
    "check": "npm-run-all prettier-check",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/main.*",
    "styleguide-server": "styleguidist server",
    "styleguide": "npm-run-all -p watch-css styleguide-server",

    "prettier-fix": "prettier --single-quote --write \"src/**/*.{js,jsx}\"",
    "prettier-check": "prettier --single-quote -l \"src/**/*.{js,jsx}\""
  },
  "homepage": ".",
  "files": [
    "build/*"
  ],
  "dependencies": {}
}
